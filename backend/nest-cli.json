{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "cities/cities.json"
      },
      {
        "include": "assets/**/*"
      }
    ],
    "watchAssets": true
  }
}